; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Sly Cooper: A Thief in Godot"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[rendering]

occlusion_culling/bvh_build_quality=1
lights_and_shadows/directional_shadow/size=2048
2d/shadow_atlas/size=1024
reflections/sky_reflections/roughness_layers=6
reflections/sky_reflections/ggx_samples=24
reflections/reflection_atlas/reflection_size=128
reflections/reflection_atlas/reflection_count=48
global_illumination/gi/use_half_resolution=true
textures/default_filters/anisotropic_filtering_level=1
environment/ssao/blur_passes=3
textures/decals/filter=2
textures/light_projectors/filter=2
occlusion_culling/occlusion_rays_per_thread=256
occlusion_culling/use_occlusion_culling=true
lights_and_shadows/positional_shadow/atlas_size=2048
